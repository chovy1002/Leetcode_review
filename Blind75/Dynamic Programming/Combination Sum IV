# O(target * len(nums))
class Solution:
    def combinationSum4(self, nums: List[int], target: int) -> int:
        dp = [0] * (target + 1)
        # base case → 空組合
        dp[0] = 1

        # 從 1 開始，一路算到 target
        for i in range(1, target + 1):
            for num in nums:
                # 確保不會越界
                if i - num >= 0:
                     # dp[i] 的組合數量 = dp[i-num] 的累加
                    dp[i] += dp[i - num]

        return dp[target]
