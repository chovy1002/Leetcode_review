class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        Max = 0
        left = 0
        right = 1

        while right < len(prices):
            curr = prices[right] - prices[left]
            if prices[left] < prices[right]:
                Max = max(Max, curr)
            else:
                left = right
            right += 1

        return Max
        
