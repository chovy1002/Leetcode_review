# O(log n) Binary Search
class Solution:
    def findMin(self, nums: List[int]) -> int:
        L = 0
        R = len(nums) - 1

        while L < R:
            mid = (L + R) // 2
            # if mid > right means the Min is on the right side
            if nums[mid] > nums[R]:
                L = mid + 1
            # else mid < right means the Min is on the left side
            else:
                R = mid

        return nums[L]
